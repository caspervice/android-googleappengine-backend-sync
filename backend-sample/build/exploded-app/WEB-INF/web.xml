<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">


    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>com.example.aramide.myapplication.backend.MyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyServlet</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletReturnBulk</servlet-name>
        <servlet-class>com.example.aramide.myapplication.backend.ServletReturnBulk</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletReturnBulk</servlet-name>
        <url-pattern>/bulkget</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>